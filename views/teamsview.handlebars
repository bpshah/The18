

<ul class="list-group">
    {{#each data}}
    <li class="list-group-item d-flex justify-content-between align-items-center">
        <img class="image-logo-teams" src="{{this.logo}}" alt="Card image cap">
        <strong>{{this.name}}</strong>
        <strong>{{this.country}}</strong>
        <a class="btn btn-outline-dark" href="./teamsfullview/{{this.id}}&{{this.name}}"><strong> > </strong></a>
    </li>
    {{/each}}
</ul>
